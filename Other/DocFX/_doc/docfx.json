{
  "metadata": [
    {
      "src": [
        {
          "src": "C:/Temp/Au/DocFX/source/",
          "files": [
            "Au.csproj"
          ]
        }
      ],
      "dest": "api",
      "shouldSkipMarkup": true,
      "filter": "filter.yml",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "memberLayout": "separatePages",
      "noRestore": true
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "editor/**.md",
          "editor/**/toc.yml",
          "cookbook/**.md",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      "images/window.png"
    ],
    "globalMetadata": {
      "_appTitle": "LibreAutomate",
      "_enableNewTab": true,
      "_disableBreadcrumb": true,
      "_disableAffix": true,
      "_disableContribution": true
    },
    "dest": "C:/Temp/Au/DocFX/site-temp",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "template1",
      "template2"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "sitemap": {
      "baseUrl": "https://www.libreautomate.com/",
      "changefreq": "weekly"
    }
  }
}

//https://dotnet.github.io/docfx/tutorial/docfx.exe_user_manual.html
